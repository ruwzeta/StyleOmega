<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.cb006789styleomega.AdminHome">



    <Button
        android:id="@+id/btnaddprod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="122dp"
        android:text="Add Product"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="113dp"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/adminSignOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign Out"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="262dp"
        android:layout_marginTop="75dp"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/btnviewinq"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/btnviewinq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View Inquiry"
        android:layout_centerVertical="true"
        android:layout_alignEnd="@+id/btnaddprod"
        tools:ignore="HardcodedText" />


</RelativeLayout>