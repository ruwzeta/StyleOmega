<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.cb006789styleomega.LoginSuccessActivity">
    <View
        android:id="@+id/title"
        android:layout_width="1000dp"
        android:layout_height="1000dp"
        android:background="#000000"
        android:alpha="0.6"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="7dp"
        app:layout_constraintHorizontal_bias="0.243" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="103dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="36dp"
        android:fontFamily="casual"
        android:text=" Welcome to Style Omega"
        android:textAlignment="center"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/tvusername"
        android:layout_width="179dp"
        android:layout_height="47dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        android:gravity="center|center_vertical"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:text="@string/emptyString" />

    <TextView
        android:id="@+id/tvHomePageHere"
        android:layout_width="226dp"
        android:layout_height="70dp"
        android:layout_marginTop="10dp"
        android:fontFamily="casual"
        android:text="Enter Shopping Area"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center|center_vertical"
        app:layout_constraintTop_toBottomOf="@+id/tvusername"
        app:layout_constraintVertical_bias="0.434"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/tvabout"
        android:layout_width="226dp"
        android:layout_height="70dp"
        android:layout_marginTop="10dp"
        android:fontFamily="casual"
        android:text="About Us"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:gravity="center|center_vertical"
        app:layout_constraintTop_toBottomOf="@+id/tvHomePageHere"
        app:layout_constraintVertical_bias="0.434"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="93dp"
        android:layout_height="68dp"
        app:srcCompat="@drawable/logo"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="146dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.476" />


</android.support.constraint.ConstraintLayout>
